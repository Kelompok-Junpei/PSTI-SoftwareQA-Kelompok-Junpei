{
  "name": "npcstore-testing",
  "version": "1.0.0",
  "description": "Automated testing for npcstore.vercel.app using Jest and CodeceptJS",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "codeceptjs run",
    "test:e2e:headless": "codeceptjs run --headless",
    "test:e2e:debug": "codeceptjs run --debug",
    "codecept:init": "codeceptjs init",
    "codecept:generate": "codeceptjs generate",
    "install:browsers": "npx playwright install chromium"
  },
  "keywords": [
    "testing",
    "jest",
    "codeceptjs",
    "e2e",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@codeceptjs/configure": "^0.10.0",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "babel-jest": "^30.0.0-beta.3",
    "codeceptjs": "^3.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.40.0",
    "puppeteer": "^21.6.0",
    "webdriverio": "^8.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.0"
  }
}
